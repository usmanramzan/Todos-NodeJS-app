{
  "name": "fp-services",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon --exec  babel-node  ./server/index.js",
    "start-prod": "babel-node ./server/index.js"
  },
  "babel": {
    "presets": [
      "es2015-node5"
    ]
  },
  "jest": {
    "verbose": false,
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "moduleNameMapper": {
      "\\.(css|less|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    }
