    "history": "^4.6.1",
    "moment": "^2.20.1",
    "morgan": "^1.9.0",
    "passport": "^0.3.2",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "sequelize": "^4.33.4",
    "sequelize-cli": "^4.0.0",
    "winston": "^2.4.1",
    "winston-daily-rotate-file": "^1.7.2"
  },
  "devDependencies": {
